#Welcome to my push swap tester hope you enjoy 
i=0
max=0
nb=0
wrong=0

make
while [ $i -le 100 ]
do
	echo -ne "\e[93m"
	cat pika | awk "NR==($i%49)"
	if [ $nb -ge $max ]
	then
		max=$nb
	fi
	if [ $nb -ge 5500 ]
	then
		wrong=$(( $wrong + 1 ))
	fi
	echo -ne "\e[0m"
	nb=$(ARG=$(seq 1 100 | shuf | tr '\n' ' ') ; ./a.out "$ARG" | wc -l)
	# echo $nb
	# echo $wrong
	i=$(( $i + 1 ))
done

echo $max
echo $wrong
echo "Dont forget to follow me on instagram"